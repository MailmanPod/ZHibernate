<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>

<class name="org.polux.domain.Engine" table="ENGINES">
	<id name="id" column="eng_id" type="long">
		<generator class="sequence">
			<param name="sequence">ENGINES_SEQ</param>
		</generator>
	</id>
	
	<property name="name" column="eng_name" type="string"></property>
	<property name="power" column="eng_power" type="int"></property>
	<property name="isERS" column="eng_is_ers" type="string"></property>
	
</class>
</hibernate-mapping>